{
  "name": "zazu-pibble",
  "description": "Get moar pibbles",
  "stylesheet": "./dist/style.css",
  "blocks": {
    "input": [
      {
        "id": "pibble",
        "type": "RootScript",
        "script": "pibble.js",
        "connections": ["Open"]
      }
    ],
    "output": [
      {
        "id": "Open",
        "type": "OpenInBrowser",
        "url": "{value}"
      }
    ]
  }
}
